package main;
import  shapesSVG.*;
import shapesSVG.Linea;
import shapesSVG.Triangulo;
public class Main{

    public static void main(String args []){
	String inicio= "<svg height='700' width='700'>";
	String cierre="</svg>";
	Vector2 vector = new Vector2(50,50);
	Vector2 vector2 = new Vector2();
		System.out.println(inicio);
	System.out.println(vector.toSVG());
	//		System.out.println(vector);
	System.out.println(vector.equals(vector2));
	Linea linea1= new Linea();
	Linea linea2= new Linea(vector,vector2);
	Linea linea3=new Linea(0,0,10,10);
		Linea linea4=new Linea(0,0,10,10);
	//	System.out.println(linea1);
	//	System.out.println(linea2);
	//System.out.println(linea3);
	System.out.println(linea1.toSVG());
	System.out.println(linea1.toSVG());
	System.out.println(linea2.toSVG());
	System.out.println(linea3.toSVG());
	System.out.println(linea3.equals(linea4));

	Rectangulo rectangulo = new Rectangulo(20,20,80,100);
	System.out.println(rectangulo.toSVG());

	Traingulo trian= new Triangulo(100,100,100,200,200,200);
	System.out.println(trian.toSVG());
		System.out.println(cierre);
    }
}
